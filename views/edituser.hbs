<h2>
    Edit User</h2>
    {{#if msg}}
    <div class="alert">{{msg}}</div>
    {{/if}}
    <center>
        {{#each rows}}
    <form method="post" action="/edituser/{{this.ID}}"autocomplete="false">
    
    <div class="form-group">

          <label for="First" class="b1">First Name</label>
          <input type="text" name="First" class="b1" value="{{this.FNAME}}">
    </div>
    <div class="form-group">
          <label for="Last">Last Name</label>
          <input type="text" name="Last" class="b2" value="{{this.LNAME}}" >
    </div>
    <div class="form-group">
          <label for="location">Location</label>
          <input type="text" name="location" class="b3" value="{{this.LOCATION}}">
    </div>
       <div class="form-group">
          <label for="email">Email</label>
          <input type="email" name="email" class="b4" value="{{this.email}}">
    </div>
       <div class="form-group">
          <label for="Dob">Dob</label>
          <input type="text" name="Dob" class="b5" value="{{this.DOB}}">
    </div>
       <div class="form-group">
          <label for="Education">Education</label>
          <input type="text" name="Education" class="b6" value="{{this.EDUCATION}}" >
    </div>
    <div class="form-group">
          <label for="about">About</label>
          <input type="text" name="about" class="b5">
     </div><br>
<div>
             <button id="two" type="submit">Save Changes </button>
             <a href="/">Back</a>
</div>

    </form>
    {{/each}}
</center>
